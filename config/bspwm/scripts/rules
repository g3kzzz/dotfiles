wid=$1
class=$2
instance=$3
consequences=$4

result() {
    eval "$consequences"
    [ "$state" ] || echo "$1"
}

case "$class" in
    *FloatingTerm*)
    	result "state=floating center=true"
    	;;

    *librewolf*|librewolf|Navigator)
        result "desktop=^3 follow=on"
        ;;
    *burp*)
        result "desktop=^4 follow=on"
        ;;
    vscodium|VSCodium)
        result "desktop=^5 follow=on"
        ;;
esac

;; Workspaces ;;;; Workspaces ;;
(deflisten workspace0 "scripts/WorkSpaces WORKSPACE")
;; Workspaces Widgets ;;
(defwidget workspace0 [] (literal :content workspace0))
;; Workspaces Main Widget ;;
(defwidget workspaces [monitor]
   (box    :orientation "v"
           :space-evenly "false"
           :valign "start"
           (box :visible { monitor=="WORKSPACE" } (workspace0))
))
